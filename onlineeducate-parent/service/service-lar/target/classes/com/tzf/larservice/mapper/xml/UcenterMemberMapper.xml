<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tzf.larservice.mapper.UcenterMemberMapper">
    <select id="getDayDataByDate" resultType="integer">
    SELECT count(*)
    FROM ucenter_member
    WHERE
    DATE(gmt_create)=#{day}
    </select>
</mapper>
